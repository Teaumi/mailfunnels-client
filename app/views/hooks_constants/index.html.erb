<p id="notice"><%= notice %></p>

<h1>Hooks Constants</h1>

<table>
	<thead>
	<tr>
		<th>Hook</th>
		<th>Campaign</th>
		<th>Campaign Jobs</th>
		<th colspan="3"></th>
	</tr>
	</thead>
	
	<tbody>
	<% @hooks_constants.each do |hooks_constant| %>
			<% Campaign.where(HookConstant_id: hooks_constant.id).find_each do |cj| %>
					<tr>
						<td>Hook: <%= hooks_constant.name %></td>
						<td>Campaign: <%= c.name %></td>
						<td>
							<% CampaignJob.where(Campaign_id: hooks_constant.id).find_each do |c| %>
								<%= c.name %> /
							<% end %>
						</td>
					</tr>
			<% end %>
	<% end %>
	</tbody>
</table>

