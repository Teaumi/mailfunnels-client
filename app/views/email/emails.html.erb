<div class="container">
	<div class="col-xs-12">
		<legend>
			<h3>Email List <%= @list.name %></h3>
			<h6><%= @list.description %></h6>
		</legend>
		<ul class="list-group">
			<% @emails.each do |e| %>
					<li class="list-group-item">
						<b><%= e.name %></b> - <%= e.email_address %>
					</li>
			<% end %>
		</ul>

	</div>

</div>